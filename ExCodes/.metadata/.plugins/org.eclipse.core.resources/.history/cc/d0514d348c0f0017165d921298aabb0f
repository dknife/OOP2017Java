
public class Triangle extends Shape {
	
	double w;
	double h;
	
	public Triangle(double w, double h) {
		this.w = w;
		this.h = h;
		type = "Triangle";
		System.out.println("Triangle Created with (w,h)=("+w+","+h+")");
	}
	
	public String toString() {
		String s = "Triangle: area = " + area();
		return s;
	}

}
