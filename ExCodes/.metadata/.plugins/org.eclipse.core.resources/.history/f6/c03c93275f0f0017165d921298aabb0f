import java.util.*;

public class Ex2017_05_01_Date {
	
	public static void main(String[] arg) {
		Scanner scanner = new Scanner(System.in);
		MyDate date;
		date = new MyDate();
		
		while(true) {
			System.out.print("Your command: 1) Set Date 2) Next Day 3) Prev Day 4) +30 days 5) -30 Days");
			int command = scanner.nextInt();
			
			switch(command) {
			case 1: break;
			case 2: break;
			case 3: break;
			case 4: break;
			case 5: break;
			default: break;
			}
		}
		
				
	}

}
