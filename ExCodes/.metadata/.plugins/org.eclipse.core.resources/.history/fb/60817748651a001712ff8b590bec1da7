import java.awt.*;
import java.awt.event.*;
import javax.swing.*;
import javax.swing.event.*;

public class Ex2017_07_01_SwingStart extends JFrame {
	
	Ex2017_07_01_SwingStart() {
		setSize(300, 300);
		setTitle("이것은 나의 윈도");
		setLocation(100, 100);
		setVisible(true);
	}
	
	public static void main(String []argv) {
		Ex2017_07_01_SwingStart f = new Ex2017_07_01_SwingStart();
		JButton comp1 = new JButton("hello");
		JButton comp2 = new JButton("world");
		JButton comp3 = new JButton("yeah");
		f.add(comp1);
		f.add(comp2);
		f.add(comp3);
		
	}

}
