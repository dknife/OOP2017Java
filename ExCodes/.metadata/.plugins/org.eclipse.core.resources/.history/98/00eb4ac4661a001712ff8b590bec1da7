import java.awt.*;
import java.awt.event.*;
import javax.swing.*;
import javax.swing.event.*;

public class Ex2017_07_02_SwingLayouts extends JFrame {
	
	Ex2017_07_02_SwingLayouts() {
		setSize(300, 300);
		setTitle("이것은 나의 윈도");
		setLocation(100, 100);
		setLayout(new FlowLayout());
		setVisible(true);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);	
	}
	
	public static void main(String []argv) {
		Ex2017_07_02_SwingLayouts f = new Ex2017_07_02_SwingLayouts();
		JButton comp1 = new JButton("hello");
		JButton comp2 = new JButton("world");
		JButton comp3 = new JButton("yeah");
		f.add(comp1);
		f.add(comp2);
		f.add(comp3);
		
	}

}
